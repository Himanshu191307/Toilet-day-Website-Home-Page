.page-banner {
    position: relative;
    color: #ffffff;
    text-align: left;
    padding: 180px 0px 175px;
    background-color: #25283a;
    z-index: 2;
}
.page-banner .image-layer {
    position: absolute;
    left: 0px;
    top: 0;
    width: 100%;
    height: 100%;
    background-position: 0px 0px;
    background-repeat: no-repeat;
    background-size: cover!important
}

.page-banner .image-layer::before {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #000000;
    opacity: 0.50;
}
.page-banner .auto-container {
    position: relative;
    z-index: 1;
}
