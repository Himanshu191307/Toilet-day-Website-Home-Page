/*** 

====================================================================
	Reset
====================================================================

 ***/

* {
	margin:0px;
	padding:0px;
	border:none;
	outline:none;
	font-size: 100%;
	line-height: inherit;
}
body {
	font-size:15px;
	color:#080808;
	line-height:23px;
	font-weight:400;
	background:#ffffff;
	-webkit-font-smoothing: antialiased;
	-moz-font-smoothing: antialiased;
	 font-family: 'Myriad Pro';
}
a {
	text-decoration:none;
	cursor:pointer;
	color: #dc4549;
}
.theme_color {
	color:#dc4549;
}
a:hover, a:focus, a:visited {
	text-decoration:none;
	outline:none;
}
h1, h2, h3, h4, h5, h6 {
	position:relative;
	font-weight:normal;
	margin:0px;
	background:none;
	line-height:1.25em;

	
}
h1,h2{  font-family: 'Premier League';}
.ui-datepicker .ui-datepicker-next {
    right: 2px;
    display: none;
}
.ui-datepicker .ui-datepicker-prev {
    left: 2px;
    display: none;
}

textarea {
	overflow:hidden;
	resize: none;
}
button {
	outline: none !important;
	cursor: pointer;
}
p {
	font-size: 16px;
	line-height:23px;
	font-weight: 400;
	margin: 0 0 15px;
}
.text {
	font-size: 15px;
	line-height: 1.7em;
	font-weight: 400;
	margin: 0 0;
}

::-webkit-input-placeholder {
color: inherit;
}
::-moz-input-placeholder {
color: inherit;
}
::-ms-input-placeholder {
color: inherit;
}
